{
  "name": "nyx-react",
  "version": "1.0.0",
  "description": "React component library",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.cjs.js",
  "type": "module",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w"
  },
  "keywords": [
    "react",
    "component",
    "aria",
    "accessiblity",
    "a11y"
  ],
  "author": "Andrea Storace",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "rollup": "^3.3.0",
    "rollup-plugin-generate-package-json": "^3.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.2",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.1.0",
    "@rollup/plugin-typescript": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-plugin-react": "^7.31.11",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/index.cjs.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.esm.js"
    },
    "./accordion": {
      "types": "./dist/types/accordion/index.d.ts",
      "node": "./dist/cjs/accordion/index.js",
      "require": "./dist/cjs/accordion/index.js",
      "default": "./dist/esm/accordion/index.js"
    },
    "./menu": {
      "types": "./dist/types/menu/index.d.ts",
      "node": "./dist/cjs/menu/index.js",
      "require": "./dist/cjs/menu/index.js",
      "default": "./dist/esm/menu/index.js"
    },
    "./listBox": {
      "types": "./dist/types/listBox/index.d.ts",
      "node": "./dist/cjs/listBox/index.js",
      "require": "./dist/cjs/listBox/index.js",
      "default": "./dist/esm/listBox/index.js"
    }
  }
}
